<?xml version="1.0" encoding="utf-8"?>
<com.vphoainha.itfmobile.view.CustomSlidingPaneLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/slidingPane"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:orientation="vertical"
	android:layout_gravity="left"
	 >

	<include layout="@layout/slidingmenu" />

	<LinearLayout
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:orientation="vertical" >

		<include layout="@layout/title_bar_fragment" />

		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:orientation="vertical" >
			
			<FrameLayout
				android:id="@+id/content"
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:layout_weight="1" >
			</FrameLayout>
		</LinearLayout>
	</LinearLayout>

</com.vphoainha.itfmobile.view.CustomSlidingPaneLayout>